{
  "name": "patchmanager-cdk",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc",
    "cdk": "npx cdk@latest",
    "ci:eslint-report": "eslint . --format html -o report.html || true",
    "clean": "rimraf dist cdk.out",
    "clean:force": "rimraf dist cdk.out node_modules",
    "synth-sam": "cdk synth --no-staging > template.yaml",
    "test": "echo no tests for this project",
    "watch": "tsc -w"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.225.0",
    "constructs": "^10.4.3"
  },
  "devDependencies": {
    "@digitraffic-cdk/digitraffic-cdk-rig": "workspace:*",
    "@digitraffic-cdk/toolchain": "workspace:*",
    "@rushstack/heft": "^1.1.5",
    "@rushstack/heft-jest-plugin": "^1.1.5",
    "@types/heft-jest": "1.0.6",
    "@types/node": "^22.19.1",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.3"
  },
  "externals": [
    "aws-sdk"
  ]
}
