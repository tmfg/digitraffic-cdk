{
  "name": "cloudfront-cdk",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prebuild": "rimraf tsd",
    "build": "tsc",
    "postbuild": "esbuild lib/lambda/**/*.ts lib/lambda/*.ts --bundle --platform=node --outdir=dist/lambda --external:aws-sdk --target=node12; node lib/lambda-versions",
    "watch": "tsc -w",
    "bundle-watch": "find lib/lambda -name '*.ts' | entr -rc npx esbuild  --bundle --platform=node lib/lambda/**/*.ts --outdir=dist/lambda --external:aws-sdk --target=node12",
    "all-watch": "parallelshell \"npm run watch\" \"npm run bundle-watch\"",
    "lint": "eslint lib/**/*.ts test/**/*.ts --fix",
    "cdk": "cdk",
    "synth-sam": "cdk synth --no-staging > template.yaml",
    "test": "echo no tests for this project"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.117.0",
    "@types/aws-lambda": "^8.10.75",
    "@types/node": "^14.14.27",
    "@types/sinon": "^10.0.0",
    "aws-cdk": "1.117.0",
    "aws-sdk": "^2.821.0",
    "esbuild": "^0.8.36",
    "parallelshell": "^3.0.2",
    "rimraf": "^3.0.2",
    "sinon": "^11.1.2",
    "ts-node": "^8.10.2",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "1.117.0",
    "@aws-cdk/aws-cloudfront": "1.117.0",
    "@aws-cdk/aws-iam": "1.117.0",
    "@aws-cdk/aws-kinesis": "1.117.0",
    "@aws-cdk/aws-lambda": "1.117.0",
    "@aws-cdk/aws-lambda-event-sources": "1.117.0",
    "@aws-cdk/aws-logs": "1.117.0",
    "@aws-cdk/aws-s3": "1.117.0",
    "@aws-cdk/aws-s3-notifications": "1.117.0",
    "@aws-cdk/aws-sqs": "1.117.0",
    "@aws-cdk/aws-wafv2": "1.117.0",
    "@aws-cdk/core": "1.117.0",
    "p-limit": "3.1.0",
    "source-map-support": "^0.5.19"
  },
  "externals": [
    "aws-sdk"
  ]
}
