{
  "name": "cloudfront",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "postbuild": "esbuild lib/lambda/**/*.ts lib/lambda/*.ts --bundle --platform=node --outdir=dist/lambda --external:aws-sdk --target=node16; ts-node lib/lambda-versions",
    "watch": "tsc -w",
    "bundle-watch": "find lib/lambda -name '*.ts' | entr -rc pnpm dlx esbuild  --bundle --platform=node lib/lambda/**/*.ts --outdir=dist/lambda --external:aws-sdk --target=node16",
    "all-watch": "parallelshell \"npm run watch\" \"npm run bundle-watch\"",
    "lint": "eslint --cache .",
    "eslint-report": "eslint . --format html -o report.html",
    "clean": "rimraf dist cdk.out",
    "clean:force": "rimraf dist cdk.out node_modules",
    "cdk": "cdk",
    "synth-sam": "cdk synth --no-staging > template.yaml",
    "test": "echo no tests for this project",
    "cdk-diff-road-test": "../../scripts/cdk-diff-and-deploy.sh road-test diff",
    "cdk-diff-road-prod": "../../scripts/cdk-diff-and-deploy.sh road-prod diff",
    "cdk-deploy-road-test": "../../scripts/cdk-diff-and-deploy.sh road-test deploy",
    "cdk-deploy-road-prod": "../../scripts/cdk-diff-and-deploy.sh road-prod deploy",
    "cdk-diff-marine-test": "../../scripts/cdk-diff-and-deploy.sh marine-test diff",
    "cdk-diff-marine-prod": "../../scripts/cdk-diff-and-deploy.sh marine-prod diff",
    "cdk-deploy-marine-test": "../../scripts/cdk-diff-and-deploy.sh marine-test deploy",
    "cdk-deploy-marine-prod": "../../scripts/cdk-diff-and-deploy.sh marine-prod deploy"
  },
  "devDependencies": {
    "@types/aws-lambda": "8.10.108",
    "@types/node": "*",
    "@types/sinon": "*",
    "aws-cdk-lib": "*",
    "aws-sdk": "2.1260.0",
    "esbuild": "*",
    "jest": "^29.3.1",
    "parallelshell": "3.0.2",
    "sinon": "*",
    "ts-node": "^10.9.1",
    "typescript": "4.9.5"
  },
  "dependencies": {
    "@digitraffic/common": "2022.11.23-5",
    "aws-cdk-lib": "2.51.1",
    "constructs": "10.1.168",
    "p-limit": "4.0.0",
    "source-map-support": "^0.5.19"
  },
  "externals": [
    "aws-sdk"
  ]
}
