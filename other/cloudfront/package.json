{
  "name": "cloudfront-cdk",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "postbuild": "esbuild lib/lambda/**/*.ts lib/lambda/*.ts --bundle --platform=node --outdir=dist/lambda --external:aws-sdk --target=node12; node lib/lambda-versions",
    "watch": "tsc -w",
    "bundle-watch": "find lib/lambda -name '*.ts' | entr -rc npx esbuild  --bundle --platform=node lib/lambda/**/*.ts --outdir=dist/lambda --external:aws-sdk --target=node12",
    "all-watch": "parallelshell \"npm run watch\" \"npm run bundle-watch\"",
    "lint": "eslint lib/**/*.ts test/**/*.ts --fix",
    "cdk": "cdk",
    "synth-sam": "cdk synth --no-staging > template.yaml",
    "test": "echo no tests for this project",
    "cdk-diff-road-test": "../../scripts/cdk-diff-and-deploy.sh road-test diff",
    "cdk-diff-road-prod": "../../scripts/cdk-diff-and-deploy.sh road-prod diff",
    "cdk-deploy-road-test": "../../scripts/cdk-diff-and-deploy.sh road-test deploy",
    "cdk-deploy-road-prod": "../../scripts/cdk-diff-and-deploy.sh road-prod deploy",
    "cdk-diff-marine-test": "../../scripts/cdk-diff-and-deploy.sh marine-test diff",
    "cdk-diff-marine-prod": "../../scripts/cdk-diff-and-deploy.sh marine-prod diff",
    "cdk-deploy-marine-test": "../../scripts/cdk-diff-and-deploy.sh marine-test deploy",
    "cdk-deploy-marine-prod": "../../scripts/cdk-diff-and-deploy.sh marine-prod deploy"
  },
  "devDependencies": {
    "@aws-cdk/assert": "*",
    "@types/aws-lambda": "*",
    "@types/node": "^14.14.27",
    "@types/sinon": "^10.0.0",
    "aws-sdk": "*",
    "esbuild": "^0.8.36",
    "parallelshell": "3.0.1",
    "sinon": "^11.1.2",
    "ts-node": "^8.10.2",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "*",
    "@aws-cdk/aws-cloudfront": "*",
    "@aws-cdk/aws-iam": "*",
    "@aws-cdk/aws-kinesis": "*",
    "@aws-cdk/aws-lambda": "*",
    "@aws-cdk/aws-lambda-event-sources": "*",
    "@aws-cdk/aws-logs": "*",
    "@aws-cdk/aws-s3": "*",
    "@aws-cdk/aws-s3-notifications": "*",
    "@aws-cdk/aws-sqs": "*",
    "@aws-cdk/aws-wafv2": "*",
    "@aws-cdk/core": "*",
    "p-limit": "3.1.0",
    "source-map-support": "^0.5.19"
  },
  "externals": [
    "aws-sdk"
  ]
}
