{
  "name": "os-key-figures",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "all-watch": "parallelshell \"npm run watch\" \"npm run bundle-watch\"",
    "build": "heft build --clean && npm run postbuild",
    "postbuild": "esbuild src/lambda/*.ts --log-level=warning --bundle --platform=node --outdir=dist/lambda --target=node22",
    "build:watch": "heft build-watch --clean",
    "bundle-watch": "find src/lambda -name '*.ts' | entr -rc npx esbuild  --bundle --platform=node src/lambda/**/*.ts --outdir=dist/lambda --target=node22",
    "cdk": "npx cdk@latest",
    "synth-sam": "cdk synth --no-staging > template.yaml",
    "test": "NODE_OPTIONS='--experimental-vm-modules' heft test --clean"
  },
  "dependencies": {
    "@aws-crypto/sha256-browser": "~5.2.0",
    "@aws-sdk/client-s3": "^3.933.0",
    "@digitraffic/common": "2025.11.11-1",
    "@smithy/node-http-handler": "~4.4.5",
    "@smithy/protocol-http": "~5.3.5",
    "@smithy/signature-v4": "~5.3.5",
    "aws-cdk-lib": "^2.233.0",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1692.0",
    "constructs": "^10.4.4",
    "ky": "^1.14.1",
    "mysql": "^2.18.1",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@aws-sdk/types": "^3.930.0",
    "@digitraffic-cdk/digitraffic-cdk-rig": "workspace:*",
    "@digitraffic-cdk/toolchain": "workspace:*",
    "@jest/globals": "^30.2.0",
    "@rushstack/heft": "^1.1.7",
    "@rushstack/heft-jest-plugin": "^1.1.7",
    "@types/aws-lambda": "8.10.159",
    "@types/heft-jest": "1.0.6",
    "@types/jest": "30.0.0",
    "@types/mysql": "2.15.27",
    "@types/node": "^22.19.1",
    "@types/sinon": "21.0.0",
    "dotenv": "~17.2.3",
    "esbuild": "0.27.2",
    "jest": "^30.2.0",
    "nock": "~14.0.10",
    "node-mocks-http": "~1.17.2",
    "parallelshell": "3.0.2",
    "sinon": "21.0.0",
    "typescript": "~5.9.3",
    "zod": "4.2.1"
  }
}
