{
  "name": "os-key-figures",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "all-watch": "parallelshell \"npm run watch\" \"npm run bundle-watch\"",
    "build": "heft build --clean && npm run postbuild",
    "postbuild": "esbuild src/lambda/*.ts --log-level=warning --bundle --platform=node --outdir=dist/lambda --target=node22",
    "build:watch": "heft build-watch --clean",
    "bundle-watch": "find src/lambda -name '*.ts' | entr -rc npx esbuild  --bundle --platform=node src/lambda/**/*.ts --outdir=dist/lambda --target=node22",
    "cdk": "cdk",
    "ci:eslint-report": "echo 'No eslint report available'",
    "synth-sam": "cdk synth --no-staging > template.yaml",
    "test": "NODE_OPTIONS='--experimental-vm-modules' heft test --clean"
  },
  "dependencies": {
    "@aws-crypto/sha256-browser": "~5.2.0",
    "@smithy/node-http-handler": "~4.0.6",
    "@smithy/protocol-http": "~5.1.2",
    "@smithy/signature-v4": "~5.1.2",
    "@aws-sdk/client-s3": "^3.837.0",
    "@digitraffic/common": "2025.6.27-2",
    "aws-cdk-lib": "^2.202.0",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1692.0",
    "ky": "^1.8.1",
    "constructs": "~10.4.2",
    "mysql": "^2.18.1",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@aws-sdk/types": "^3.821.0",
    "@digitraffic-cdk/digitraffic-cdk-rig": "workspace:*",
    "@digitraffic/eslint-config": "workspace:*",
    "@digitraffic-cdk/toolchain": "workspace:*",
    "@rushstack/eslint-config": "^3.7.1",
    "@rushstack/heft": "^0.74.0",
    "@rushstack/heft-jest-plugin": "^0.16.9",
    "@types/aws-lambda": "8.10.150",
    "@types/heft-jest": "1.0.6",
    "@types/jest": "30.0.0",
    "@types/mysql": "2.15.27",
    "@types/node": "22.15.2",
    "@types/sinon": "17.0.4",
    "@jest/globals": "^30.0.3",
    "dotenv": "~16.5.0",
    "esbuild": "0.25.5",
    "eslint": "^8.57.0",
    "jest": "^30.0.3",
    "nock": "~14.0.5",
    "node-mocks-http": "~1.17.2",
    "parallelshell": "3.0.2",
    "sinon": "21.0.0",
    "typescript": "~5.8.3",
    "zod": "~3.25.67"
  }
}
