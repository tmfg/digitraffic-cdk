{
  "name": "es-key-figures",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "postbuild": "esbuild lib/lambda/*.ts --bundle --platform=node --outdir=dist/lambda --external:aws-sdk --target=node14",
    "watch": "tsc -w",
    "bundle-watch": "find lib/lambda -name '*.ts' | entr -rc npx esbuild  --bundle --platform=node lib/lambda/**/*.ts --outdir=dist/lambda --external:aws-sdk --target=node14",
    "all-watch": "parallelshell \"npm run watch\" \"npm run bundle-watch\"",
    "test": "echo no tests for this project",
    "cdk": "cdk",
    "synth-sam": "cdk synth --no-staging > template.yaml"
  },
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "@types/node": "*",
    "@types/sinon": "*",
    "esbuild": "^0.8.36",
    "jest": "^27.0.6",
    "parallelshell": "3.0.1",
    "sinon": "*",
    "ts-jest": "^27.0.4",
    "ts-node": "^8.10.2",
    "typescript": "*"
  },
  "dependencies": {
    "aws-cdk-lib": "*",
    "constructs": "*",
    "@types/aws-lambda": "*",
    "@types/mysql": "^2.15.15",
    "aws-lambda": "^1.0.6",
    "axios": "^0.21.1",
    "mysql": "^2.18.1",
    "source-map-support": "^0.5.19"
  },
  "externals": [
    "aws-sdk"
  ]
}
