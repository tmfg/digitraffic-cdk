{
  "name": "es-key-figures",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "postbuild": "esbuild lib/lambda/*.ts --bundle --platform=node --outdir=dist/lambda --external:aws-sdk --target=node14",
    "watch": "tsc -w",
    "bundle-watch": "find lib/lambda -name '*.ts' | entr -rc pnpm dlx esbuild  --bundle --platform=node lib/lambda/**/*.ts --outdir=dist/lambda --external:aws-sdk --target=node14",
    "all-watch": "parallelshell \"npm run watch\" \"npm run bundle-watch\"",
    "test": "echo no tests for this project",
    "cdk": "cdk",
    "synth-sam": "cdk synth --no-staging > template.yaml"
  },
  "devDependencies": {
    "@types/jest": "^29.2.3",
    "@types/node": "*",
    "@types/sinon": "*",
    "esbuild": "^0.15.15",
    "jest": "^29.3.1",
    "parallelshell": "3.0.2",
    "sinon": "*",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "*"
  },
  "dependencies": {
    "@types/aws-lambda": "8.10.108",
    "@types/mysql": "^2.15.15",
    "aws-cdk-lib": "2.51.1",
    "aws-lambda": "^1.0.6",
    "axios": "^1.2.0",
    "constructs": "10.1.168",
    "mysql": "^2.18.1",
    "source-map-support": "^0.5.19"
  },
  "externals": [
    "aws-sdk"
  ]
}
