{
  "name": "sse-sea-state-estimate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prebuild": "json2ts -i lib/schemas/ -o lib/generated/",
    "build": "tsc",
    "postbuild": "esbuild lib/lambda/**/*.ts --bundle --platform=node --outdir=dist/lambda --external:aws-sdk --target=node12",
    "watch": "tsc -w",
    "bundle-watch": "find lib/lambda -name '*.ts' | entr -rc npx esbuild  --bundle --platform=node lib/lambda/**/*.ts --outdir=dist/lambda --external:aws-sdk --target=node12",
    "all-watch": "parallelshell \"npm run watch\" \"npm run bundle-watch\"",
    "lint": "eslint lib/**/*.ts test/**/*.ts --fix",
    "pretest": "json2ts -i lib/schemas/ -o lib/generated/",
    "test": "jest --runInBand --detectOpenHandles",
    "cdk": "cdk",
    "synth-sam": "cdk synth --no-staging > template.yaml",
    "cdk-diff-test": "../../scripts/cdk-diff-and-deploy.sh marine-test diff",
    "cdk-diff-prod": "../../scripts/cdk-diff-and-deploy.sh marine-prod diff",
    "cdk-deploy-test": "../../scripts/cdk-diff-and-deploy.sh marine-test deploy",
    "cdk-deploy-prod": "../../scripts/cdk-diff-and-deploy.sh marine-prod deploy"
  },
  "devDependencies": {
    "@types/aws-lambda": "*",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.27",
    "@types/sinon": "^10.0.0",
    "@types/xml2js": "^0.4.8",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "aws-sdk": "*",
    "esbuild": "^0.8.36",
    "eslint": "^7.13.0",
    "jest": "^27.0.6",
    "jest-junit": "^12.0.0",
    "json-schema-to-typescript": "^10.1.4",
    "parallelshell": "3.0.1",
    "sinon": "^11.1.2",
    "ts-jest": "^27.0.4",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "*",
    "@aws-cdk/aws-cloudwatch": "*",
    "@aws-cdk/aws-cloudwatch-actions": "*",
    "@aws-cdk/aws-ec2": "*",
    "@aws-cdk/aws-events-targets": "*",
    "@aws-cdk/aws-iam": "*",
    "@aws-cdk/aws-lambda": "*",
    "@aws-cdk/aws-lambda-event-sources": "*",
    "@aws-cdk/aws-pinpoint": "*",
    "@aws-cdk/aws-sns": "*",
    "@aws-cdk/core": "*",
    "@middy/core": "^1.2.0",
    "@types/ramda": "0.27.44",
    "ajv-cli": "^3.3.0",
    "dtsgenerator": "^3.3.1",
    "moment": "*",
    "moment-timezone": "*",
    "pg-native": "*",
    "pg-promise": "*",
    "ramda": "0.27.1",
    "source-map-support": "^0.5.19"
  },
  "externals": [
    "aws-sdk"
  ]
}
