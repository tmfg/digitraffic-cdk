{
  "name": "shiplight",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prebuild": "rimraf tsd",
    "build": "tsc",
    "postbuild": "esbuild lib/lambda/**/*.ts --bundle --platform=node --outdir=dist/lambda --external:aws-sdk --target=node12",
    "watch": "tsc -w",
    "bundle-watch": "find lib/lambda -name '*.ts' | entr -rc npx esbuild  --bundle --platform=node lib/lambda/**/*.ts --outdir=dist/lambda --external:aws-sdk --target=node12",
    "all-watch": "parallelshell \"npm run watch\" \"npm run bundle-watch\"",
    "lint": "eslint lib/**/*.ts test/**/*.ts --fix",
    "test": "echo no tests for this project",
    "cdk": "cdk",
    "synth-sam": "cdk synth --no-staging > template.yaml"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.75",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.27",
    "@types/sinon": "^10.0.0",
    "@types/xml2js": "^0.4.8",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "aws-sdk": "^2.821.0",
    "esbuild": "^0.8.36",
    "eslint": "^7.13.0",
    "jest": "^26.6.2",
    "jest-junit": "^12.0.0",
    "parallelshell": "^3.0.2",
    "rimraf": "^3.0.2",
    "sinon": "^10.0.0",
    "ts-jest": "^26.1.4",
    "ts-node": "^8.10.2",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "1.111.0",
    "@aws-cdk/aws-cloudwatch": "1.111.0",
    "@aws-cdk/aws-cloudwatch-actions": "1.111.0",
    "@aws-cdk/aws-ec2": "1.111.0",
    "@aws-cdk/aws-events": "1.111.0",
    "@aws-cdk/aws-events-targets": "1.111.0",
    "@aws-cdk/aws-iam": "1.111.0",
    "@aws-cdk/aws-lambda": "1.111.0",
    "@aws-cdk/aws-lambda-event-sources": "1.111.0",
    "@aws-cdk/aws-logs": "1.111.0",
    "@aws-cdk/aws-secretsmanager": "1.111.0",
    "@aws-cdk/core": "1.111.0",
    "@types/ramda": "0.27.1",
    "axios": "^0.21.1",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.31",
    "pg-native": "3.0.0",
    "pg-promise": "10.3.5",
    "pg-query-stream": "4.0.0",
    "ramda": "0.27.1",
    "source-map-support": "^0.5.19",
    "uuid": "^8.3.1"
  }
}
