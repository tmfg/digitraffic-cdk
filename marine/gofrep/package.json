{
  "name": "gofrep",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "tsc && npm run postbuild",
    "postbuild": "esbuild lib/lambda/**/*.ts --log-level=warning --bundle --platform=node --outdir=dist/lambda --external:aws-sdk --target=node16",
    "cdk": "npx cdk@latest",
    "ci:eslint-report": "eslint . --format html -o report.html || true",
    "clean": "rimraf dist cdk.out",
    "clean:force": "rimraf dist cdk.out node_modules",
    "lint": "eslint --cache .",
    "synth-sam": "cdk synth --no-staging > template.yaml",
    "test": "jest --detectOpenHandles",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@digitraffic/common": "2023.9.14-1",
    "aws-cdk-lib": "~2.103.1",
    "aws-sdk": "~2.1482.0",
    "axios": "^1.6.0",
    "constructs": "10.3.0",
    "moment": "^2.29.4",
    "moment-timezone": "0.5.43",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@digitraffic-cdk/eslint-config": "workspace:*",
    "@digitraffic-cdk/toolchain": "workspace:*",
    "@rushstack/eslint-config": "^3.4.1",
    "@types/aws-lambda": "8.10.125",
    "@types/jest": "29.5.6",
    "@types/node": "20.8.9",
    "@types/sinon": "10.0.20",
    "@types/xml2js": "0.4.13",
    "esbuild": "0.19.5",
    "eslint": "~8.52.0",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "parallelshell": "3.0.2",
    "sinon": "17.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "~4.9.5"
  },
  "externals": [
    "aws-sdk"
  ]
}
