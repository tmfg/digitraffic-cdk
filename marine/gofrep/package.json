{
  "name": "gofrep",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "postbuild": "esbuild lib/lambda/**/*.ts --bundle --platform=node --outdir=dist/lambda --external:aws-sdk --target=node14",
    "watch": "tsc -w",
    "lint": "eslint --cache .",
    "eslint-report": "eslint . --format html -o report.html",
    "clean": "rimraf dist cdk.out",
    "clean:force": "rimraf dist cdk.out node_modules",
    "test": "jest --detectOpenHandles",
    "cdk": "cdk",
    "synth-sam": "cdk synth --no-staging > template.yaml"
  },
  "devDependencies": {
    "@aws-cdk/assert": "*",
    "@types/aws-lambda": "*",
    "@types/jest": "^26.0.22",
    "@types/node": "*",
    "@types/sinon": "*",
    "@types/xml2js": "^0.4.8",
    "esbuild": "^0.8.36",
    "jest": "^27.0.6",
    "jest-junit": "^13.0.0",
    "parallelshell": "3.0.1",
    "sinon": "*",
    "ts-jest": "^27.0.4",
    "ts-node": "^8.10.2",
    "typescript": "*"
  },
  "dependencies": {
    "@digitraffic/common": "2022.10.6-1",
    "aws-cdk-lib": "*",
    "aws-sdk": "*",
    "axios": "^0.21.1",
    "constructs": "*",
    "moment": "*",
    "moment-timezone": "*",
    "ramda": "0.27.1",
    "source-map-support": "^0.5.19"
  },
  "externals": [
    "aws-sdk"
  ]
}
