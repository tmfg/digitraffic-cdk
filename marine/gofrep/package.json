{
  "name": "gofrep",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "postbuild": "esbuild lib/lambda/**/*.ts --bundle --platform=node --outdir=dist/lambda --external:aws-sdk --target=node12",
    "watch": "tsc -w",
    "lint": "eslint lib/**/*.ts test/**/*.ts --fix",
    "test": "jest --detectOpenHandles",
    "cdk": "cdk",
    "synth-sam": "cdk synth --no-staging > template.yaml"
  },
  "devDependencies": {
    "@aws-cdk/assert": "*",
    "@types/aws-lambda": "*",
    "@types/jest": "^26.0.22",
    "@types/node": "*",
    "@types/sinon": "^10.0.0",
    "@types/xml2js": "^0.4.8",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "esbuild": "^0.8.36",
    "eslint": "^7.13.0",
    "jest": "^27.0.6",
    "jest-junit": "^13.0.0",
    "parallelshell": "3.0.1",
    "sinon": "^11.1.2",
    "ts-jest": "^27.0.4",
    "ts-node": "^8.10.2",
    "typescript": "*"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "*",
    "@aws-cdk/aws-lambda": "*",
    "@aws-cdk/core": "*",
    "aws-sdk": "*",
    "axios": "^0.21.1",
    "moment": "*",
    "moment-timezone": "*",
    "ramda": "0.27.1",
    "source-map-support": "^0.5.19"
  },
  "externals": [
    "aws-sdk"
  ]
}
