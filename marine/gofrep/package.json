{
  "name": "gofrep",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "tsc && npm run postbuild",
    "postbuild": "esbuild lib/lambda/**/*.ts --log-level=warning --bundle --platform=node --outdir=dist/lambda --external:aws-sdk --target=node20",
    "cdk": "npx cdk@latest",
    "ci:eslint-report": "eslint . --format html -o report.html || true",
    "clean": "rimraf dist cdk.out",
    "clean:force": "rimraf dist cdk.out node_modules",
    "lint": "eslint --cache .",
    "synth-sam": "cdk synth --no-staging > template.yaml",
    "test": "jest --detectOpenHandles",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@digitraffic/common": "2024.1.10-1",
    "aws-cdk-lib": "~2.118.0",
    "aws-sdk": "~2.1531.0",
    "axios": "^1.6.5",
    "constructs": "10.3.0",
    "moment": "^2.30.1",
    "moment-timezone": "0.5.44",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@digitraffic-cdk/eslint-config": "workspace:*",
    "@digitraffic-cdk/toolchain": "workspace:*",
    "@rushstack/eslint-config": "^3.5.1",
    "@types/aws-lambda": "8.10.131",
    "@types/jest": "29.5.11",
    "@types/node": "20.10.7",
    "@types/sinon": "17.0.2",
    "@types/xml2js": "0.4.14",
    "esbuild": "0.19.11",
    "eslint": "~8.56.0",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "parallelshell": "3.0.2",
    "sinon": "17.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "~5.3.3"
  },
  "externals": [
    "aws-sdk"
  ]
}
