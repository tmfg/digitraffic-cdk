{
  "name": "gofrep",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "tsc && npm run postbuild",
    "postbuild": "esbuild lib/lambda/**/*.ts --log-level=warning --bundle --platform=node --outdir=dist/lambda --external:aws-sdk --target=node16",
    "cdk": "npx cdk@latest",
    "ci:eslint-report": "eslint . --format html -o report.html || true",
    "clean": "rimraf dist cdk.out",
    "clean:force": "rimraf dist cdk.out node_modules",
    "lint": "eslint --cache .",
    "synth-sam": "cdk synth --no-staging > template.yaml",
    "test": "jest --detectOpenHandles",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@digitraffic/common": "2023.9.14-1",
    "aws-cdk-lib": "~2.85.0",
    "aws-sdk": "~2.1405.0",
    "axios": "^1.3.6",
    "constructs": "10.2.61",
    "moment": "^2.29.4",
    "moment-timezone": "0.5.43",
    "ramda": "~0.29.0",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@digitraffic-cdk/eslint-config": "workspace:*",
    "@digitraffic-cdk/toolchain": "workspace:*",
    "@rushstack/eslint-config": "^3.3.2",
    "@types/aws-lambda": "~8.10.119",
    "@types/jest": "^29.5.2",
    "@types/node": "18.15.13",
    "@types/sinon": "10.0.15",
    "@types/xml2js": "^0.4.11",
    "esbuild": "0.18.10",
    "eslint": "~8.43.0",
    "jest": "^29.5.0",
    "jest-junit": "^16.0.0",
    "parallelshell": "3.0.2",
    "sinon": "15.2.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "~4.9.5"
  },
  "externals": [
    "aws-sdk"
  ]
}
