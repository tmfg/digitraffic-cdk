{
  "name": "aton-faults",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "all-watch": "parallelshell \"npm run watch\" \"npm run bundle-watch\"",
    "build": "tsc && npm run postbuild",
    "postbuild": "npm run compile-lambdas && npm run compile-canaries",
    "bundle-watch": "find lib/lambda -name '*.ts' | entr -rc npx esbuild  --bundle --platform=node lib/lambda/**/*.ts --outdir=dist/lambda --external:aws-sdk --target=node16",
    "cdk": "npx cdk@latest",
    "cdk-deploy-marine-prod": "../../scripts/cdk-diff-and-deploy.sh marine-prod deploy",
    "cdk-deploy-marine-test": "../../scripts/cdk-diff-and-deploy.sh marine-test deploy",
    "cdk-diff-marine-prod": "../../scripts/cdk-diff-and-deploy.sh marine-prod diff",
    "cdk-diff-marine-test": "../../scripts/cdk-diff-and-deploy.sh marine-test diff",
    "ci:eslint-report": "eslint . --format html -o report.html || true",
    "clean": "rimraf dist cdk.out",
    "clean:force": "rimraf dist cdk.out node_modules",
    "compile-canaries": "esbuild lib/canaries/*.ts --log-level=warning --bundle --platform=node --outdir=dist/nodejs/node_modules --external:aws-sdk --external:Synthetics --target=node16",
    "compile-lambdas": "esbuild lib/lambda/**/*.ts --log-level=warning --bundle --platform=node --outdir=dist/lambda --external:aws-sdk --target=node16",
    "lint": "eslint --cache .",
    "synth-sam": "cdk synth --no-staging > template.yaml",
    "test": "jest --detectOpenHandles --forceExit",
    "test_errors": "npm test 2>&1 | grep 'FAIL'",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@digitraffic/common": "2023.9.14-1",
    "@middy/core": "^4.6.5",
    "@middy/sqs-partial-batch-failure": "^4.6.5",
    "@turf/boolean-disjoint": "^6.5.0",
    "@turf/buffer": "^6.5.0",
    "@turf/helpers": "^6.5.0",
    "aws-cdk-lib": "~2.103.1",
    "aws-sdk": "~2.1482.0",
    "axios": "^1.6.0",
    "constructs": "10.3.0",
    "date-fns": "~2.30.0",
    "date-fns-tz": "~2.0.0",
    "parse-link-header": "^2.0.0",
    "pg-native": "^3.0.1",
    "pg-promise": "^11.5.4",
    "source-map-support": "^0.5.21",
    "spex": "^3.3.0",
    "uuid": "^9.0.1",
    "wkx": "^0.5.0",
    "xml2js": "^0.6.2",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@digitraffic-cdk/eslint-config": "workspace:*",
    "@digitraffic-cdk/toolchain": "workspace:*",
    "@rushstack/eslint-config": "^3.4.1",
    "@types/aws-lambda": "8.10.125",
    "@types/geojson": "7946.0.12",
    "@types/jest": "29.5.6",
    "@types/node": "20.8.9",
    "@types/sinon": "10.0.20",
    "@types/uuid": "9.0.6",
    "@types/xml2js": "0.4.13",
    "@types/xsd-schema-validator": "0.5.6",
    "esbuild": "0.19.5",
    "eslint": "~8.52.0",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "parallelshell": "3.0.2",
    "sinon": "17.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "~4.9.5",
    "xsd-schema-validator": "^0.7.0"
  },
  "externals": [
    "aws-sdk"
  ]
}
