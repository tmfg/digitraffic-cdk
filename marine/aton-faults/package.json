{
  "name": "aton-faults-cdk",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prebuild": "rimraf tsd",
    "build": "tsc",
    "postbuild": "esbuild lib/lambda/**/*.ts --bundle --platform=node --outdir=dist/lambda --external:aws-sdk --target=node12",
    "watch": "tsc -w",
    "bundle-watch": "find lib/lambda -name '*.ts' | entr -rc npx esbuild  --bundle --platform=node lib/lambda/**/*.ts --outdir=dist/lambda --external:aws-sdk --target=node12",
    "all-watch": "parallelshell \"npm run watch\" \"npm run bundle-watch\"",
    "lint": "eslint lib/**/*.ts test/**/*.ts --fix",
    "test": "jest --detectOpenHandles",
    "cdk": "cdk",
    "synth-sam": "cdk synth --no-staging > template.yaml"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.111.0",
    "@types/aws-lambda": "^8.10.75",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.27",
    "@types/sinon": "^10.0.0",
    "@types/xml2js": "^0.4.8",
    "@types/xsd-schema-validator": "^0.5.4",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "esbuild": "^0.8.36",
    "eslint": "^7.13.0",
    "jest": "^26.6.2",
    "jest-junit": "^12.0.0",
    "parallelshell": "^3.0.2",
    "rimraf": "^3.0.2",
    "sinon": "^10.0.0",
    "ts-jest": "^26.1.4",
    "ts-node": "^8.10.2",
    "typescript": "^4.1.3",
    "xsd-schema-validator": "^0.6.0"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "1.111.0",
    "@aws-cdk/aws-ec2": "1.111.0",
    "@aws-cdk/aws-events-targets": "1.111.0",
    "@aws-cdk/aws-lambda": "1.111.0",
    "@aws-cdk/aws-sns": "1.111.0",
    "@aws-cdk/aws-sns-subscriptions": "1.111.0",
    "@aws-cdk/core": "1.111.0",
    "@types/geojson": "^7946.0.7",
    "aws-sdk": "^2.821.0",
    "axios": "^0.21.1",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.31",
    "parse-link-header": "^1.0.1",
    "pg-native": "3.0.0",
    "pg-promise": "10.3.5",
    "pg-query-stream": "4.0.0",
    "source-map-support": "^0.5.19",
    "wkx": "^0.5.0",
    "xml2js": "^0.4.23"
  },
  "externals": [
    "aws-sdk"
  ]
}
