{
  "name": "portactivity",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "heft build --clean && npm run build:bundle",
    "build:bundle": "npm run compile-lambdas && npm run compile-canaries",
    "build:tsc": "heft build --clean",
    "build:watch": "heft build-watch --clean",
    "cdk": "cdk",
    "clean": "rimraf dist cdk.out",
    "clean:force": "rimraf dist cdk.out node_modules",
    "compile-canaries": "esbuild src/canaries/*.ts --log-level=warning --bundle --platform=node --outdir=dist/nodejs/node_modules --external:aws-sdk --external:Synthetics --target=node22",
    "compile-lambdas": "node lib/esbuild.js",
    "compile-lambdas-old": "esbuild src/lambda/**/*.ts --log-level=warning --bundle --platform=node --outdir=dist/lambda --target=node24",
    "synth-sam": "cdk synth --no-staging > template.yaml",
    "test": "NODE_OPTIONS='--experimental-vm-modules' heft test --clean",
    "test:watch": "NODE_OPTIONS='--experimental-vm-modules' heft test-watch --clean"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.965.0",
    "@aws-sdk/client-sns": "3.965.0",
    "@aws-sdk/client-sqs": "3.965.0",
    "@aws-sdk/client-ssm": "3.965.0",
    "@date-fns/tz": "1.4.1",
    "@digitraffic/common": "2026.1.23-1",
    "@middy/core": "6.4.5",
    "@middy/sqs-partial-batch-failure": "6.4.5",
    "amqplib": "0.10.7",
    "aws-cdk-lib": "2.234.0",
    "constructs": "10.4.4",
    "date-fns": "4.1.0",
    "ky": "1.14.2",
    "lodash": "4.17.23",
    "pg-native": "3.5.2",
    "pg-promise": "12.3.0",
    "spex": "4.1.0",
    "uuid": "13.0.0",
    "ws": "8.19.0",
    "xml2js": "0.6.2"
  },
  "devDependencies": {
    "@digitraffic-cdk/cdk-rig": "workspace:*",
    "@digitraffic-cdk/esbuild": "workspace:*",
    "@digitraffic-cdk/testing": "workspace:*",
    "@digitraffic-cdk/toolchain": "workspace:*",
    "@jest/globals": "30.2.0",
    "@rushstack/heft": "1.1.7",
    "@rushstack/heft-jest-plugin": "1.1.7",
    "@types/amqplib": "0.10.8",
    "@types/aws-lambda": "8.10.159",
    "@types/heft-jest": "1.0.6",
    "@types/jest": "30.0.0",
    "@types/lodash": "4.17.23",
    "@types/node": "24.10.4",
    "@types/sinon": "21.0.0",
    "@types/ws": "8.18.1",
    "@types/xml2js": "0.4.14",
    "aws-cdk": "2.1100.2",
    "esbuild": "0.27.2",
    "jest": "30.2.0",
    "jest-junit": "16.0.0",
    "parallelshell": "3.0.2",
    "ts-jest": "29.4.6",
    "typescript": "5.9.3"
  }
}
