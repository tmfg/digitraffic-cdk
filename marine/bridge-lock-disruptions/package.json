{
  "name": "bridge-lock-disruptions",
  "version": "0.1.0",
  "bin": {
    "bridge-lock-disruptions": "bin/bridge-lock-disruptions-app.js"
  },
  "scripts": {
    "prebuild": "rimraf tsd",
    "build": "tsc",
    "postbuild": "parcel build lib/lambda/**/*.ts --target=node --global handler -d dist/lambda --bundle-node-modules --no-minify --public-url dist/lambda",
    "watch": "tsc -w",
    "parcel-watch": "parcel watch lib/lambda/**/*.ts --target=node --global handler -d dist/lambda --bundle-node-modules",
    "watch-bundle": "parallelshell \"npm run watch\" \"npm run parcel-watch\"",
    "lint": "eslint lib/**/*.ts test/**/*.ts --fix",
    "test": "jest --runInBand --detectOpenHandles",
    "cdk": "cdk",
    "synth-sam": "cdk synth --no-staging > template.yaml"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.82.0",
    "@types/aws-lambda": "^8.10.59",
    "@types/jest": "^26.0.8",
    "@types/node": "^14.0.27",
    "@types/sinon": "^9.0.4",
    "@types/xml2js": "^0.4.5",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "eslint": "^7.13.0",
    "jest": "^26.2.2",
    "parallelshell": "3.0.1",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-externals": "^0.5.1",
    "rimraf": "^3.0.2",
    "sinon": "^9.0.2",
    "ts-jest": "^26.1.4",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "1.82.0",
    "@aws-cdk/aws-events-targets": "1.82.0",
    "@aws-cdk/core": "1.82.0",
    "@aws-cdk/aws-secretsmanager": "1.82.0",
    "@types/geojson": "^7946.0.7",
    "@types/moment": "^2.13.0",
    "axios": "^0.21.1",
    "geojson-validation": "^1.0.2",
    "parse-link-header": "^1.0.1",
    "pg-native": "3.0.0",
    "pg-promise": "10.8.6",
    "pg-query-stream": "4.0.0",
    "source-map-support": "^0.5.19",
    "wkx": "^0.5.0",
    "xml2js": "v0.4.23"
  },
  "externals": [
    "aws-sdk"
  ]
}
