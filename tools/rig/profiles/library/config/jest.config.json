{
  "extends": "@rushstack/heft-jest-plugin/includes/jest-shared.config.json",
  "collectCoverage": true,
  "roots": ["<rootDir>/dist"],
  "testMatch": ["<rootDir>/dist/__test__/**/*.test.{js,tsx}"],
  "modulePathIgnorePatterns": ["<rootDir>/src"],
  "maxWorkers": "1",
  "coverageThreshold": {
    "global": {
      "branches": 50,
      "functions": 50,
      "lines": 50,
      "statements": 50
    }
  }
}
