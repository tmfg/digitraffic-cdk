    "typescript": "*"
  },
  "dependencies": {
    "@digitraffic/common": "2022.11.2-1",
    "aws-cdk-lib": "*",
    "aws-sdk": "*",
    "constructs": "*"
  },
  "externals": [
    "aws-sdk"
  ]
}
