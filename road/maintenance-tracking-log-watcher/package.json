{
  "name": "maintenance-tracking-watch",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prebuild": "rimraf tsd",
    "build": "tsc",
    "postbuild": "esbuild lib/lambda/*.ts --bundle --platform=node --outdir=dist/lambda --external:aws-sdk --target=node12",
    "watch": "tsc -w",
    "bundle-watch": "find lib/lambda -name '*.ts' | entr -rc npx esbuild  --bundle --platform=node lib/lambda/**/*.ts --outdir=dist/lambda --external:aws-sdk --target=node12",
    "all-watch": "parallelshell \"npm run watch\" \"npm run bundle-watch\"",
    "lint": "eslint lib/**/*.ts test/**/*.ts --fix",
    "test": "jest --runInBand --detectOpenHandles",
    "cdk": "cdk",
    "synth-sam": "cdk synth --no-staging > template.yaml"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.75",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.27",
    "@types/sinon": "^10.0.0",
    "@types/xml2js": "^0.4.8",
    "aws-sdk": "^2.821.0",
    "esbuild": "^0.8.36",
    "jest": "^26.6.2",
    "jest-junit": "^12.0.0",
    "node-fetch": "^2.6.1",
    "parallelshell": "^3.0.2",
    "rimraf": "^3.0.2",
    "sinon": "^10.0.0",
    "ts-jest": "^26.1.4",
    "ts-node": "^8.10.2",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@aws-cdk/aws-events": "1.111.0",
    "@aws-cdk/aws-events-targets": "1.111.0",
    "@aws-cdk/aws-iam": "1.111.0",
    "@aws-cdk/aws-s3": "1.111.0",
    "@aws-cdk/aws-sns": "1.111.0",
    "@aws-cdk/aws-sns-subscriptions": "1.111.0",
    "@aws-cdk/core": "1.111.0",
    "@types/node-fetch": "^2.5.7",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.31",
    "source-map-support": "^0.5.19",
    "uuid": "^8.3.1"
  },
  "externals": [
    "aws-sdk"
  ]
}
