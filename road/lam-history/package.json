{
  "name": "lam-history",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "all-watch": "parallelshell \"npm run watch\" \"npm run bundle-watch\"",
    "build": "tsc && npm run postbuild",
    "postbuild": "esbuild lib/lambda/**/*.ts --log-level=warning --bundle --platform=node --outdir=dist/lambda --external:aws-sdk --target=node16",
    "bundle-watch": "find lib/lambda -name '*.ts' | entr -rc npx esbuild  --bundle --platform=node lib/lambda/**/*.ts --outdir=dist/lambda --external:aws-sdk --target=node16",
    "cdk": "cdk",
    "cdk-deploy-road-prod": "../../scripts/cdk-diff-and-deploy.sh road-prod deploy LamHistoryProd",
    "cdk-deploy-road-test": "../../scripts/cdk-diff-and-deploy.sh road-test deploy LamHistoryTest",
    "cdk-diff-road-prod": "../../scripts/cdk-diff-and-deploy.sh road-prod diff LamHistoryProd",
    "cdk-diff-road-test": "../../scripts/cdk-diff-and-deploy.sh road-test diff LamHistoryTest",
    "ci:eslint-report": "eslint . --format html -o report.html || true",
    "clean": "rimraf dist cdk.out",
    "clean:force": "rimraf dist cdk.out node_modules",
    "lint": "eslint --cache .",
    "synth-sam": "cdk synth --no-staging > template.yaml",
    "test": "jest --detectOpenHandles --forceExit",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@digitraffic/common": "2023.2.13-1",
    "aws-cdk-lib": "~2.76.0",
    "aws-sdk": "~2.1362.0",
    "axios": "^1.3.6",
    "constructs": "10.2.2"
  },
  "devDependencies": {
    "@aws-cdk/assert": "2.68.0",
    "@digitraffic-cdk/eslint-config": "workspace:*",
    "@digitraffic-cdk/toolchain": "workspace:*",
    "@rushstack/eslint-config": "^3.2.0",
    "@types/aws-lambda": "~8.10.114",
    "@types/jest": "^29.5.1",
    "@types/node": "18.15.13",
    "@types/sinon": "10.0.14",
    "@types/xml2js": "^0.4.11",
    "esbuild": "0.17.17",
    "eslint": "~8.38.0",
    "jest": "^29.5.0",
    "jest-junit": "^16.0.0",
    "sinon": "15.0.4",
    "ts-jest": "^29.1.0",
    "typescript": "~4.9.5"
  }
}
