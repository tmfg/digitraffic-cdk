{
  "name": "variable-signs",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc && npm run build:bundle",
    "build:bundle": "npm run compile-lambdas && npm run compile-canaries",
    "cdk": "cdk",
    "cdk-deploy-road-prod": "../../scripts/cdk-diff-and-deploy.sh road-prod deploy",
    "cdk-deploy-road-test": "../../scripts/cdk-diff-and-deploy.sh road-test deploy",
    "cdk-diff-road-prod": "../../scripts/cdk-diff-and-deploy.sh road-prod diff",
    "cdk-diff-road-test": "../../scripts/cdk-diff-and-deploy.sh road-test diff",
    "clean": "rimraf dist cdk.out",
    "clean:force": "rimraf dist cdk.out node_modules",
    "compile-canaries": "esbuild src/canaries/*.ts --log-level=warning --bundle --platform=node --outdir=dist/nodejs/node_modules --external:Synthetics --target=node22",
    "compile-lambdas": "node lib/esbuild.js",
    "compile-lambdas-old": "esbuild src/lambda/**/*.ts --log-level=warning --bundle --platform=node --outdir=dist/lambda --out-extension:.js=.mjs --format=esm --target=node22 --banner:js=\"import { createRequire } from 'module'; const require = createRequire(import.meta.url);\"",
    "synth-sam": "cdk synth --no-staging > template.yaml",
    "test": "NODE_OPTIONS='--experimental-vm-modules' heft test --clean",
    "test:watch": "NODE_OPTIONS='--experimental-vm-modules' heft test-watch --clean"
  },
  "dependencies": {
    "@digitraffic/common": "2026.2.5-3",
    "@rgrove/parse-xml": "4.2.0",
    "aws-cdk-lib": "2.236.0",
    "constructs": "10.4.5",
    "pg-native": "3.5.2",
    "pg-promise": "12.3.0"
  },
  "devDependencies": {
    "@aws-sdk/client-api-gateway": "3.975.0",
    "@digitraffic-cdk/cdk-rig": "workspace:*",
    "@digitraffic-cdk/esbuild": "workspace:*",
    "@digitraffic-cdk/testing": "workspace:*",
    "@digitraffic-cdk/toolchain": "workspace:*",
    "@jest/globals": "30.2.0",
    "@rushstack/heft": "1.1.10",
    "@rushstack/heft-jest-plugin": "1.1.10",
    "@types/aws-lambda": "8.10.160",
    "@types/heft-jest": "1.0.6",
    "@types/jest": "30.0.0",
    "@types/node": "25.0.10",
    "@types/xml2js": "0.4.14",
    "aws-cdk": "2.1103.0",
    "esbuild": "0.27.2",
    "jest": "30.2.0",
    "jest-junit": "16.0.0",
    "parallelshell": "3.0.2",
    "spex": "4.1.0",
    "ts-jest": "29.4.6",
    "typescript": "5.9.3"
  }
}
