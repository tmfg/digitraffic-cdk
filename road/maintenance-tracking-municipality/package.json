{
  "name": "maintenance-tracking-municipality",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "all-watch": "parallelshell \"npm run watch\" \"npm run bundle-watch\"",
    "build": "tsc && npm run postbuild",
    "postbuild": "npm run compile-lambdas && npm run compile-canaries",
    "bundle-watch": "find lib/lambda -name '*.ts' | entr -rc npx esbuild  --bundle --platform=node lib/lambda/**/*.ts --outdir=dist/lambda --external:aws-sdk --target=node16",
    "cdk": "cdk",
    "cdk-deploy-road-prod": "../../scripts/cdk-diff-and-deploy.sh road-prod deploy",
    "cdk-deploy-road-test": "../../scripts/cdk-diff-and-deploy.sh road-test deploy",
    "cdk-diff-road-prod": "../../scripts/cdk-diff-and-deploy.sh road-prod diff",
    "cdk-diff-road-test": "../../scripts/cdk-diff-and-deploy.sh road-test diff",
    "cdk-synth-road-prod": "cdk synth MaintenanceTrackingMunicipalityRoadProd --no-staging > template-road-prod.yaml",
    "cdk-synth-road-test": "cdk synth MaintenanceTrackingMunicipalityRoadTest --no-staging > template-road-test.yaml",
    "ci:eslint-report": "eslint . --format html -o report.html || true",
    "clean": "rimraf dist cdk.out",
    "clean:force": "rimraf dist cdk.out node_modules",
    "compile-canaries": "../../scripts/check-ts-files-exists-and-run.sh 'lib/canaries' 'esbuild lib/canaries/*.ts --log-level=warning --bundle --platform=node --outdir=dist/nodejs/node_modules --external:aws-sdk --external:Synthetics --target=node16'",
    "compile-lambdas": "esbuild lib/lambda/**/*.ts --log-level=warning --bundle --platform=node --outdir=dist/lambda --external:aws-sdk --target=node16",
    "lint": "eslint --cache .",
    "synth-sam": "cdk synth --no-staging > template.yaml",
    "test": "jest --detectOpenHandles --forceExit",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@digitraffic/common": "2023.5.25-1",
    "aws-cdk-lib": "^2.81.0",
    "axios": "^1.4.0",
    "constructs": "10.2.37",
    "date-fns": "^2.30.0",
    "geojson": "^0.5.0",
    "pg-native": "^3.0.1",
    "pg-promise": "^11.5.0",
    "source-map-support": "^0.5.21",
    "spex": "^3.3.0"
  },
  "devDependencies": {
    "@digitraffic-cdk/eslint-config": "workspace:*",
    "@digitraffic-cdk/toolchain": "workspace:*",
    "@rushstack/eslint-config": "^3.3.0",
    "@types/aws-lambda": "^8.10.115",
    "@types/geojson": "^7946.0.10",
    "@types/jest": "^29.5.1",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.2.5",
    "@types/sinon": "^10.0.15",
    "esbuild": "^0.17.19",
    "eslint": "^8.41.0",
    "jest": "^29.5.0",
    "jest-junit": "^16.0.0",
    "lodash": "^4.17.21",
    "maintenance-tracking": "workspace:maintenance-tracking@*",
    "parallelshell": "^3.0.2",
    "sinon": "^15.1.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "~4.9.5"
  }
}
