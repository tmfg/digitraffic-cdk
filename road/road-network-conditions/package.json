{
  "name": "road-network-conditions",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "cdk": "cdk",
    "ci:eslint-report": "eslint . --format html -o report.html || true",
    "clean": "rimraf dist cdk.out",
    "clean:force": "rimraf dist cdk.out node_modules",
    "compile-lambdas": "esbuild lib/lambda/**/*.ts --log-level=warning --bundle --platform=node --outdir=dist/lambda --external:aws-sdk --target=node16",
    "deploy": "npm compile-lambdas && npx cdk@latest deploy",
    "diff": "npm compile-lambdas && npx cdk@latest diff",
    "lint": "eslint --cache .",
    "nolambda": "nodemon -e ts --exec 'npm compile-lambdas'",
    "synth-sam": "cdk synth --no-staging > template.yaml",
    "test": "jest --detectOpenHandles",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@digitraffic/common": "2023.6.27-1",
    "aws-cdk-lib": "~2.85.0",
    "axios": "^1.3.6",
    "constructs": "10.2.61",
    "pg-native": "^3.0.1",
    "pg-promise": "^11.4.3",
    "ramda": "~0.29.0"
  },
  "devDependencies": {
    "@digitraffic-cdk/eslint-config": "workspace:*",
    "@digitraffic-cdk/toolchain": "workspace:*",
    "@rushstack/eslint-config": "^3.3.2",
    "@types/aws-lambda": "~8.10.119",
    "@types/geojson": "^7946.0.10",
    "@types/jest": "^29.5.2",
    "@types/node": "18.15.13",
    "@types/ramda": "~0.29.0",
    "esbuild": "0.18.10",
    "eslint": "~8.43.0",
    "jest": "^29.5.0",
    "jest-junit": "^16.0.0",
    "nodemon": "^2.0.22",
    "ts-jest": "^29.1.0",
    "types-ramda": "~0.29.1",
    "typescript": "~4.9.5"
  },
  "externals": [
    "aws-sdk"
  ]
}
