{
  "name": "digitraffic-cdk",
  "version": "1.1.0",
  "main": "index.js",
  "engines": {
    "node": ">=16"
  },
  "repository": "git@github.com:tmfg/digitraffic-cdk.git",
  "author": "Digitraffic <digitraffic@fintraffic.fi>",
  "private": true,
  "pnpm": {
    "overrides": {
      "typescript": "^4.7.4"
    }
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "esbuild": "^0.17.8",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-deprecation": "1.3.3",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "nx": "15.5.1",
    "prettier": "^2.8.4",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.5"
  },
  "lint-staged": {
    "*.{js,ts,css,md,yml,yaml,json}": "prettier --write"
  },
  "scripts": {
    "build:all": "nx run-many --target=build",
    "build:affected": "nx affected:build",
    "test:all": "nx run-many --target=test",
    "test:affected": "nx affected:test",
    "lint:affected": "nx affected:lint",
    "eslint-report:all": "nx run-many --target=eslint-report",
    "eslint-report:affected": "nx affected --target=eslint-report",
    "clean:all": "nx run-many --target=clean",
    "clean:all:force": "nx run-many --target=clean:force && rimraf node_modules",
    "prepare": "husky install"
  }
}
