{
  "name": "digitraffic-cdk",
  "version": "1.1.0",
  "main": "index.js",
  "engines": {
    "node": ">=14",
    "yarn": ">1.2 <2"
  },
  "repository": "git@github.com:tmfg/digitraffic-cdk.git",
  "author": "Digitraffic <digitraffic@fintraffic.fi>",
  "private": true,
  "workspaces": [
    "marine/*",
    "road/*",
    "aviation/*",
    "other/*"
  ],
  "resolutions": {
    "@types/aws-lambda": "8.10.102",
    "@types/sinon": "^10.0.13",
    "aws-cdk-lib": "2.38.1",
    "aws-sdk": "2.1198.0",
    "constructs": "10.1.80",
    "moment": "2.29.2",
    "moment-timezone": "0.5.34",
    "pg-native": "~3.0.1",
    "pg-promise": "10.11.1",
    "sinon": "^14.0.1",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.18.0",
    "esbuild": "^0.14.34",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": ">=6",
    "lint-staged": ">=10",
    "nx": "14.8.3",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4"
  },
  "lint-staged": {
    "*.{js,ts,css,md,yml,yaml,json}": "prettier --write"
  },
  "scripts": {
    "build:all": "nx run-many --target=build",
    "build:affected": "nx affected:build",
    "test:all": "nx run-many --target=test",
    "test:affected": "nx affected:test",
    "lint:affected": "nx affected:lint",
    "eslint-report:all": "nx run-many --target=eslint-report",
    "eslint-report:affected": "nx affected:eslint-report",
    "clean:all": "nx run-many --target=clean",
    "clean:all:force": "nx run-many --target=clean:force && rimraf node_modules",
    "prepare": "husky install"
  }
}
