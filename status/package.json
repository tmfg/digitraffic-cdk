{
  "name": "status-cdk",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prebuild": "rimraf tsd",
    "build": "tsc",
    "postbuild": "esbuild lib/lambda/**/*.ts --bundle --platform=node --outdir=dist/lambda --external:aws-sdk --target=node12",
    "watch": "tsc -w",
    "bundle-watch": "find lib/lambda -name '*.ts' | entr -rc npx esbuild  --bundle --platform=node lib/lambda/**/*.ts --outdir=dist/lambda --external:aws-sdk --target=node12",
    "all-watch": "parallelshell \"npm run watch\" \"npm run bundle-watch\"",
    "lint": "eslint lib/**/*.ts test/**/*.ts --fix",
    "cdk": "cdk",
    "synth-sam": "cdk synth --no-staging > template.yaml",
    "test": "echo no tests for this project"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.107.0",
    "@types/aws-lambda": "^8.10.75",
    "@types/node": "^14.14.27",
    "@types/sinon": "^10.0.0",
    "aws-cdk": "1.83.0",
    "jest": "^26.6.2",
    "parallelshell": "^3.0.2",
    "esbuild": "^0.8.36",
    "querystring": "^0.2.1",
    "rimraf": "^3.0.2",
    "sinon": "^10.0.0",
    "ts-node": "^8.10.2",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "1.107.0",
    "@aws-cdk/aws-events-targets": "1.107.0",
    "@aws-cdk/aws-lambda": "1.107.0",
    "@aws-cdk/aws-secretsmanager": "1.107.0",
    "@aws-cdk/core": "1.107.0",
    "@types/paho-mqtt": "^1.0.4",
    "aws-sdk": "^2.821.0",
    "axios": "^0.21.1",
    "paho-mqtt": "^1.1.0",
    "source-map-support": "^0.5.19",
    "ws": "^7.4.1"
  },
  "externals": [
    "aws-sdk"
  ]
}
